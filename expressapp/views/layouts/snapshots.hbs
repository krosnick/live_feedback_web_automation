<div id="snapshotLineInfo">
    <span id="lineNumberContainer">
        <span id="currentLineNumber">
            <div id="elementForFlashing"></div>
            <span>Snapshots for line number: </span>
            <span id="lineNumber"></span>
        </span>
        <span>
            <label for="lineNumInputField">Set line number:</label>
            <input id="lineNumInputField" type="number" name="lineNumInputField" min="1" required>
            <span class="validity"></span>
        </span>
    </span>
    <br>
    <button id="lockLineNumberButton" class="clickableButton"><b>Lock</b> line number</button>
    <button id="unLockLineNumberButton" class="clickableButton"><b>Unlock</b> line number</button>
    <button id="pinSnapshotsButton" class="clickableButton"><b>Pin</b> snapshots</button>
    <button id="unPinSnapshotsButton" class="clickableButton"><b>Unpin</b> snapshots</button>
</div>
<p id="noSnapshots">
    No snapshots
</p>
<div id="container">

</div>
<div id="editorBrowserViewID" editorBrowserViewID="{{editorBrowserViewID}}"></div>